<h1>Wikis</h1>

<div class="row">
  <div class="col-md-8">
    <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success'%>
  </div>
</div>

<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |w| %>
        <div class="media-heading">
          <h4 class="media-body">
            <%= link_to w.title, wiki_path(w) %>
          <% end %>
        </h4>
      </div>
    </div>
  </div>
